{
  "name": "fortune-cookie-ai",
  "version": "1.0.0",
  "description": "AI-powered Fortune Cookie Generator with SEO optimization",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "./app/globals.css",
    "./lib/global-error-handler.ts",
    "./lib/service-worker.ts"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:deployment": "node scripts/test-deployment.js",
    "test:local": "TEST_URL=http://localhost:3000 node scripts/test-deployment.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "analyze": "ANALYZE=true npm run build",
    "vercel-build": "npm run type-check && npm run build",
    "postbuild": "echo 'Build completed successfully!'",
    "vercel-check": "node scripts/vercel-deployment-check.js",
    "deploy-check": "npm run vercel-check",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:seed:clean": "tsx scripts/seed-database.ts --clean",
    "db:seed:force": "tsx scripts/seed-database.ts --force",
    "db:validate": "tsx scripts/seed-database.ts validate",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:api": "jest --testPathPattern=api",
    "test:components": "jest --testPathPattern=components",
    "test:lib": "jest --testPathPattern=lib"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/uuid": "^10.0.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.0.0",
    "input-otp": "^1.4.2",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "prisma": "^6.14.0",
    "react": "^18.3.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.4",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.20.5",
    "typescript": "^5.4.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "keywords": [
    "fortune-cookie",
    "ai-generator",
    "next.js",
    "react",
    "seo",
    "typescript",
    "framer-motion",
    "shadcn-ui"
  ],
  "author": "Fortune Cookie AI Team",
  "license": "MIT"
}
