{
  "name": "fortune-cookie-ai",
  "version": "1.0.0",
  "description": "AI-powered Fortune Cookie Generator with SEO optimization",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "./app/globals.css",
    "./lib/global-error-handler.ts",
    "./lib/service-worker.ts"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:deployment": "node scripts/test-deployment.js",
    "test:local": "TEST_URL=http://localhost:3000 node scripts/test-deployment.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "analyze": "ANALYZE=true npm run build",
    "vercel-build": "npm run type-check && npm run build",
    "postbuild": "echo 'Build completed successfully!'",
    "vercel-check": "node scripts/vercel-deployment-check.js",
    "deploy-check": "npm run vercel-check",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:seed:clean": "tsx scripts/seed-database.ts --clean",
    "db:seed:force": "tsx scripts/seed-database.ts --force",
    "db:validate": "tsx scripts/seed-database.ts validate",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:api": "jest --testPathPattern=api",
    "test:components": "jest --testPathPattern=components",
    "test:lib": "jest --testPathPattern=lib"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/uuid": "^10.0.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/blob": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "google-auth-library": "^10.5.0",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.7",
    "next-mdx-remote": "^5.0.0",
    "prisma": "^6.14.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.38",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.20.5",
    "typescript": "^5.4.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "keywords": [
    "fortune-cookie",
    "ai-generator",
    "next.js",
    "react",
    "seo",
    "typescript",
    "framer-motion",
    "shadcn-ui"
  ],
  "author": "Fortune Cookie AI Team",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "last 2 versions",
      "Chrome >= 90",
      "Firefox >= 88",
      "Safari >= 14",
      "Edge >= 90"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
